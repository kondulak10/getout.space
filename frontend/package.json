		"@fortawesome/react-fontawesome": "^3.1.0",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@mapbox/polyline": "^1.2.1",
		"@radix-ui/react-alert-dialog": "^1.1.15",
		"@radix-ui/react-dialog": "^1.1.15",
