		"canvas-confetti": "^1.9.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"graphql": "^16.11.0",
		"h3-js": "^4.3.0",
		"mapbox-gl": "^3.16.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
